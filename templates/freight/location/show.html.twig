{% extends 'base.html.twig' %}

{% block title %}Location{% endblock %}

{% block body %}
    <h1>Location</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ location.id }}</td>
            </tr>
            <tr>
                <th>ZipCode</th>
                <td>{{ location.zipCode }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ location.city }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ location.address }}</td>
            </tr>
            <tr>
                <th>State</th>
                <td>{{ location.state }}</td>
            </tr>
            <tr>
                <th>CompanyName</th>
                <td>{{ location.companyName }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ location.phone }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ location.comment }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ location.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_freight_location_index') }}">back to list</a>

    <a href="{{ path('app_freight_location_edit', {'id': location.id}) }}">edit</a>

    {{ include('freight/location/_delete_form.html.twig') }}
{% endblock %}
